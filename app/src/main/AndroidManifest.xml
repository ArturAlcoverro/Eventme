<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidprog2.eventme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Eventme">
        <activity android:name=".presentation.activities.UserFriendsActivity"></activity>
        <activity android:name=".presentation.activities.UserEventsActivity" />
        <activity android:name=".presentation.activities.EditProfileActivity" />
        <activity android:name=".presentation.activities.ChatActivity" />
        <activity android:name=".presentation.activities.EventDetailActivity" />
        <activity android:name=".presentation.activities.RegisterActivity"
            android:noHistory="true"/>
        <activity android:name=".presentation.activities.LoginActivity"
            android:noHistory="true"/>
        <activity android:name=".presentation.activities.StartActivity"
            android:noHistory="true"/>
        <activity android:name=".presentation.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>